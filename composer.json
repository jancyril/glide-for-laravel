{
    "name": "jancyril/glide-for-laravel",
    "description": "A wrapper for League/Glide to easily manipulate images in Laravel 5",
    "homepage": "https://github.com/jancyril/glide-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Cyril Segubience",
            "email": "jancyril@segubience.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "league/glide": "^1.0",
        "illuminate/support": "^5.1",
        "illuminate/filesystem": "^5.1"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "illuminate/container": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Jancyril\\Glide\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jancyril\\Glide\\Test\\": "tests"
        }
    }
}
